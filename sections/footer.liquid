        </div>
        <div class="container">
            <div class="copyright-wrap">
                <div class="row gy-2 align-items-center justify-content-between">
                    <div class="col-md-auto wow fadeInUp" data-wow-delay="0.4s">
                        <p class="copyright">Â© {{ section.settings.copyright_text}}</p>
                    </div>
                    <div class="col-md-auto wow fadeInUp" data-wow-delay="0.5s">
                        <div class="footer-menu">
                            <ul>
                              {% for link in linklists[section.settings.footer_linklist_four].links %}
                                <li><a href="{{link.url}}">{{ link.title}}</li>
                              {% endfor %}
                            </ul>
                        </div>
                    </div>
                </div>
            </div>

        </div>
    </footer>

{%- if section.settings.enable_country_selector or section.settings.enable_language_selector -%}
  <script src="{{ 'localization-form.js' | asset_url }}" defer="defer"></script>
{%- endif -%}

